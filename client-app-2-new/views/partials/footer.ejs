<script>
    window.onscroll = function () {
        myFunction()
    };

    function myFunction() {
        if (document.body.scrollTop > 100 || document.documentElement.scrollTop > 100) {
            $('#custom-navbar').addClass('scrolled');
        } else {
            $('#custom-navbar').removeClass('scrolled');
        }
    }

}
